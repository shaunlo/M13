@{
    ViewData["Title"] = "Home Page";
}

@model List<Bowler>

<h1>@ViewData["TeamName"]</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                BowlerLastName
            </th>
            <th>
                BowlerFirstName
            </th>
            <th>
                BowlerMiddleInit
            </th>
            <th>
                BowlerAddress
            </th>
            <th>
                BowlerCity
            </th>
            <th>
                BowlerState
            </th>
            <th>
                BowlerZip
            </th>
            <th>
                BowlerPhoneNumber
            </th>
            <th>
                TeamID
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var b in Model)
        {
            <tr>
                <td>
                    @b.BowlerLastName
                </td>
                <td>
                    @b.BowlerFirstName
                </td>
                <td>
                    @b.BowlerMiddleInit
                </td>
                <td>
                    @b.BowlerAddress
                </td>
                <td>
                    @b.BowlerCity
                </td>
                <td>
                    @b.BowlerState
                </td>
                <td>
                    @b.BowlerZip
                </td>
                <td>
                    @b.BowlerPhoneNumber
                </td>
                <td>
                    @b.TeamID
                </td>
            </tr>
        }
    </tbody>
</table>

